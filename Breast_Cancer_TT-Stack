e","source":"# Part-38: Helper function to extract logits and labels\n\nimport numpy as np\n\ndef extract_logits_and_labels(model, data_loader, device):\n    \"\"\"\n    Runs the model over data_loader and returns:\n    - logits_all: np.array [N, num_classes]\n    - labels_all: np.array [N]\n    \"\"\"\n    model.eval()\n    logits_list = []\n    labels_list = []\n\n    with torch.no_grad():\n        for images, labels in data_loader:\n            images = images.to(device)\n            labels = labels.to(device)\n\n            outputs = model(images)      # logits [B, 2]\n            logits_list.append(outputs.cpu().numpy())\n            labels_list.append(labels.cpu().numpy())\n\n    logits_all = np.concatenate(logits_list, axis=0)\n    labels_all = np.concatenate(labels_list, axis=0)\n\n    return logits_all, labels_all\n","metadata":{"tags":[]},"outputs":[],"execution_count":44},{"cell_type":"code","source":"# Part-39 (fixed): Build meta-train and meta-val features for stacking ensemble\n# using a single pass over each loader (no label mismatches).\n\nfrom collections import OrderedDict\n\n# Ensure deterministic order of models\nensemble_models = OrderedDict(ensemble_models)\nmodel_items = list(ensemble_models.items())\nnum_models = len(model_items)\n\nprint(\"Models in ensemble (in order):\", [name for name, _ in model_items])\n\n# ---------- META-TRAIN FEATURES ----------\n\ntrain_feature_lists = [ [] for _ in range(num_models) ]\nmeta_train_labels_list = []\n\nprint(\"\\nBuilding meta-train features...\")\nfor batch_idx, (images, labels) in enumerate(train_loader):\n    images = images.to(device)\n    labels = labels.cpu().numpy()\n    meta_train_labels_list.append(labels)\n\n    with torch.no_grad():\n        for idx, (name, mdl) in enumerate(model_items):\n            mdl.eval()\n            outputs = mdl(images)                   # logits [B, 2]\n            train_feature_lists[idx].append(\n                outputs.cpu().numpy()\n            )\n\n# Concatenate over all batches\ny_train_meta = np.concatenate(meta_train_labels_list, axis=0)\nmeta_train_features = [\n    np.concatenate(feat_list, axis=0) for feat_list in train_feature_lists\n]\n\n# Concatenate along feature dimension: [N, 2 * num_models]\nX_train_meta = np.concatenate(meta_train_features, axis=1)\n\nprint(\"Meta-train feature shape:\", X_train_meta.shape)\nprint(\"Meta-train labels shape:\", y_train_meta.shape)\n\n# ---------- META-VAL FEATURES ----------\n\nval_feature_lists = [ [] for _ in range(num_models) ]\nmeta_val_labels_list = []\n\nprint(\"\\nBuilding meta-val features...\")\nfor batch_idx, (images, labels) in enumerate(val_loader):\n    images = images.to(device)\n    labels = labels.cpu().numpy()\n    meta_val_labels_list.append(labels)\n\n    with torch.no_grad():\n        for idx, (name, mdl) in enumerate(model_items):\n            mdl.eval()\n            outputs = mdl(images)                   # logits [B, 2]\n            val_feature_lists[idx].append(\n                outputs.cpu().numpy()\n            )\n\ny_val_meta = np.concatenate(meta_val_labels_list, axis=0)\nmeta_val_features = [\n    np.concatenate(feat_list, axis=0) for feat_list in val_feature_lists\n]\n\nX_val_meta = np.concatenate(meta_val_features, axis=1)\n\nprint(\"Meta-val feature shape:\", X_val_meta.shape)\nprint(\"Meta-val labels shape:\", y_val_meta.shape)\n","metadata":{"tags":[]},"outputs":[{"name":"stdout","output_type":"stream","text":["Models in ensemble (in order): ['RepViT', 'DaViT', 'EfficientViT', 'MobileViT', 'MViT', 'PVTv2', 'FasterViT']\n","\n","Building meta-train features...\n","Meta-train feature shape: (992, 14)\n","Meta-train labels shape: (992,)\n","\n","Building meta-val features...\n","Meta-val feature shape: (149, 14)\n","Meta-val labels shape: (149,)\n"]}],"execution_count":45},{"cell_type":"code","source":"# Part-40: Train stacking ensemble meta-classifier\n\nfrom sklearn.linear_model import LogisticRegression\nfrom sklearn.preprocessing import StandardScaler\nfrom sklearn.pipeline import Pipeline\nfrom sklearn.metrics import (\n    accuracy_score, precision_score, recall_score, f1_score,\n    roc_auc_score, classification_report\n)\n\n# Pipeline: standardize features -> logistic regression\nensemble_clf = Pipeline([\n    (\"scaler\", StandardScaler()),\n    (\"logreg\", LogisticRegression(\n        max_iter=500,\n        class_weight=\"balanced\",\n        n_jobs=-1\n    ))\n])\n\nensemble_clf.fit(X_train_meta, y_train_meta)\nprint(\"TT-Stack meta-classifier trained.\")\n","metadata":{"tags":[]},"outputs":[{"name":"stdout","output_type":"stream","text":["TT-Stack meta-classifier trained.\n"]}],"execution_count":46},{"cell_type":"code","source":"# Part-41: Evaluate TT-Stack ensemble on meta-val\n\nfrom sklearn.metrics import roc_curve, confusion_matrix\nimport matplotlib.pyplot as plt\nimport seaborn as sns\n\n# Predict probabilities for class 1 (Cancer)\nprobs_val_ensemble = ensemble_clf.predict_proba(X_val_meta)[:, 1]\npreds_val_ensemble = (probs_val_ensemble >= 0.5).astype(int)\n\nacc_ens  = accuracy_score(y_val_meta, preds_val_ensemble)\nprec_ens = precision_score(y_val_meta, preds_val_ensemble)\nrec_ens  = recall_score(y_val_meta, preds_val_ensemble)\nf1_ens   = f1_score(y_val_meta, preds_val_ensemble)\nroc_ens  = roc_auc_score(y_val_meta, probs_val_ensemble)\n\nprint(\"\\n=== TT-Stack Ensemble Validation Metrics ===\")\nprint(f\"Accuracy:  {acc_ens:.4f}\")\nprint(f\"Precision: {prec_ens:.4f}\")\nprint(f\"Recall:    {rec_ens:.4f}\")\nprint(f\"F1-Score:  {f1_ens:.4f}\")\nprint(f\"ROC-AUC:   {roc_ens:.4f}\")\n\nprint(\"\\nClassification report (ensemble):\")\nprint(classification_report(y_val_meta, preds_val_ensemble,\n                            target_names=[\"Non-Cancer\", \"Cancer\"]))\n\n# --- ROC curve ---\nfpr_ens, tpr_ens, _ = roc_curve(y_val_meta, probs_val_ensemble)\n\nplt.figure(figsize=(6, 5))\nplt.plot(fpr_ens, tpr_ens, label=f\"TT-Stack AUC = {roc_ens:.4f}\")\nplt.plot([0, 1], [0, 1], \"k--\", label=\"Random\")\nplt.xlabel(\"False Positive Rate\")\nplt.ylabel(\"True Positive Rate\")\nplt.title(\"ROC Curve - TT-Stack Ensemble\")\nplt.legend(loc=\"lower right\")\nplt.grid(True)\nplt.tight_layout()\nplt.savefig(os.path.join(OUTPUT_DIR, \"tt_stack_roc_curve.png\"))\nplt.close()\n\n# --- Confusion Matrix ---\ncm_ens = confusion_matrix(y_val_meta, preds_val_ensemble)\ncm_df_ens = pd.DataFrame(\n    cm_ens,\n    index=[\"True Non-Cancer\", \"True Cancer\"],\n    columns=[\"Pred Non-Cancer\", \"Pred Cancer\"]\n)\n\nplt.figure(figsize=(5, 4))\nsns.heatmap(cm_df_ens, annot=True, fmt=\"d\", cmap=\"Blues\")\nplt.title(\"Confusion Matrix - TT-Stack Ensemble\")\nplt.ylabel(\"True label\")\nplt.xlabel(\"Predicted label\")\nplt.tight_layout()\nplt.savefig(os.path.join(OUTPUT_DIR, \"tt_stack_confusion_matrix.png\"))\nplt.close()\n\nprint(f\"\\nTT-Stack ROC and confusion matrix plots saved to {OUTPUT_DIR}\")\n","metadata":{"tags":[]},"outputs":[{"name":"stdout","output_type":"stream","text":["\n","=== TT-Stack Ensemble Validation Metrics ===\n","Accuracy:  0.9933\n","Precision: 1.0000\n","Recall:    0.9600\n","F1-Score:  0.9796\n","ROC-AUC:   0.9997\n","\n","Classification report (ensemble):\n","              precision    recall  f1-score   support\n","\n","  Non-Cancer       0.99      1.00      1.00       124\n","      Cancer       1.00      0.96      0.98        25\n","\n","    accuracy                           0.99       149\n","   macro avg       1.00      0.98      0.99       149\n","weighted avg       0.99      0.99      0.99       149\n","\n","\n","TT-Stack ROC and confusion matrix plots saved to /mnt/disk2/Breast_Cancer/Mammogram Mastery A Robust Dataset for Breast Cancer Detection and Medical Education/Breast Cancer Dataset/Augmented Dataset\n"]}],"execution_count":47},{"cell_type":"code","source":"","metadata":{},"outputs":[],"execution_count":null}]}